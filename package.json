{
  "name": "yu-gi-oh-card-editor",
  "displayName": "Yu-Gi-Oh! Card Editor",
  "version": "2.1.6",
  "description": "Un Ã©diteur de cartes Yu-Gi-Oh",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathisNadin/Yu-Gi-Oh-Card-Editor.git"
  },
  "author": {
    "name": "Mathis Nadin",
    "email": "mathis.nadin@gmail.com"
  },
  "license": "MIT",
  "main": "./.build-desktop/main.js",
  "dbName": "card-editor-db",
  "objectStoreName": "card-editor-object-store",
  "presets": {
    "development": {
      "apiUrl": ""
    },
    "production": {
      "apiUrl": ""
    }
  },
  "scripts": {
    "start": "npm run start:desktop",
    "build": "npm run build:desktop",
    "watch:client-apis": "node scripts/copy-api-to-clients.js",
    "watch:interfaces": "node scripts/build-api-interfaces.js",
    "watch:dev": "concurrently \"npm:watch:interfaces\" \"npm:watch:client-apis\"",
    "watch:server": "nodemon --watch source/ --exec \"ts-node -r tsconfig-paths/register source/server/index.ts\" -e ts,json",
    "watch:api": "concurrently \"npm:watch:server\" \"npm:watch:interfaces\" \"npm:watch:client-apis\"",
    "start:desktop-preload": "cross-env NODE_ENV=development PLATFORM=desktop-preload TS_NODE_TRANSPILE_ONLY=true webpack --config webpack/index.ts",
    "start:desktop-main": "cross-env NODE_ENV=development PLATFORM=desktop-main electron scripts/start-desktop-main-dev.js",
    "start:desktop-renderer": "cross-env NODE_ENV=development PLATFORM=desktop-renderer webpack serve --config webpack/index.ts",
    "start:desktop-watch": "concurrently \"npm run start:desktop-renderer\" \"npm run start:desktop-main\"",
    "start:desktop": "cross-env NODE_ENV=development npm-run-all --serial start:desktop-preload start:desktop-watch",
    "build:desktop-main": "cross-env NODE_ENV=production PLATFORM=desktop-main TS_NODE_TRANSPILE_ONLY=true webpack --config webpack/index.ts",
    "build:desktop-renderer": "cross-env NODE_ENV=production PLATFORM=desktop-renderer TS_NODE_TRANSPILE_ONLY=true webpack --config webpack/index.ts",
    "build:desktop": "cross-env NODE_ENV=production node scripts/clean-build-desktop.js && concurrently \"npm run build:desktop-main\" \"npm run build:desktop-renderer\"",
    "compile:desktop": "cross-env NODE_ENV=production npm run build:desktop && node scripts/clean-package-desktop.js && electron-builder build --publish never",
    "package:desktop": "cross-env NODE_ENV=production npm run build:desktop && node scripts/clean-package-desktop.js && electron-builder build --publish always",
    "start:web-standalone": "cross-env NODE_ENV=development PLATFORM=web-standalone webpack serve --config webpack/index.ts",
    "build:web-standalone": "cross-env NODE_ENV=production PLATFORM=web-standalone webpack --config webpack/index.ts",
    "watch:web": "cross-env NODE_ENV=development PLATFORM=web-served webpack --watch --config webpack/webpack.config.ts",
    "build:web-served": "cross-env NODE_ENV=production PLATFORM=web-served webpack --config webpack/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix \"./source/**/*.{js,jsx,ts,tsx,json}\" \"./scripts/**/*.{js,jsx,ts,tsx,json}\" \"./webpack/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./source/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"./scripts/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"./webpack/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "validate": "npm run format && npm run lint && npm run typecheck",
    "list-dependencies": "node scripts/list-dependencies.mjs",
    "sync-dependencies": "node scripts/sync-dependencies.mjs",
    "update-changelog": "node scripts/update-changelog.mjs",
    "bump": "node scripts/bump-version.mjs"
  },
  "dependencies": {
    "electron-dl": "^3.5.2",
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "@capacitor-community/sqlite": "^6.0.2",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/network": "^6.0.0",
    "@capacitor/push-notifications": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/csp-html-webpack-plugin": "^3.0.5",
    "@types/node": "^18.15.7",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/semver": "^7.5.8",
    "@types/webpack": "^5.28.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "axios": "^1.7.4",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.9.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "electronmon": "^2.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "glob": "^11.0.0",
    "html-webpack-plugin": "^5.6.0",
    "html2canvas": "^1.4.1",
    "husky": "^4.3.8",
    "mini-css-extract-plugin": "^2.8.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-image-crop": "^11.0.7",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.24.1",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "semver": "^7.6.3",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}